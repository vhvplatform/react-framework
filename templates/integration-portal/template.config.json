{
  "name": "integration-portal",
  "description": "A developer portal template with API documentation, explorer, and analytics",
  "version": "1.0.0",
  "source": {
    "repo": "https://github.com/longvhv/Integrationhubdeveloperportal",
    "branch": "main"
  },
  "components": {
    "required": ["Dashboard", "APIExplorer", "Documentation", "Navigation"],
    "optional": ["Analytics", "Settings", "Profile"]
  },
  "routes": [
    {
      "path": "/",
      "component": "Dashboard",
      "protected": true,
      "layout": "MainLayout"
    },
    {
      "path": "/api-explorer",
      "component": "APIExplorer",
      "protected": true,
      "layout": "MainLayout"
    },
    {
      "path": "/docs",
      "component": "Documentation",
      "protected": false,
      "layout": "MainLayout"
    }
  ],
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "react-hook-form": "^7.49.2",
    "recharts": "^2.10.3",
    "lucide-react": "^0.294.0",
    "date-fns": "^3.0.6",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "modules": ["api-explorer", "documentation", "analytics"],
  "customization": {
    "theme": true,
    "layout": true,
    "auth": true
  }
}
